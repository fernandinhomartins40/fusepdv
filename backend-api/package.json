{
  "name": "pdv-backend-api",
  "version": "1.0.0",
  "description": "Backend API para Sistema PDV com importação de NF-e",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsup src",
    "start": "node dist/server.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts"
  },
  "keywords": [
    "pdv",
    "nfe",
    "fastify",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/jwt": "^8.0.0",
    "@fastify/multipart": "^8.1.0",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/swagger": "^9.6.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@prisma/client": "^5.7.1",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.3.1",
    "fast-xml-parser": "^4.3.3",
    "fastify": "^4.25.2",
    "fastify-socket.io": "^5.0.0",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.2",
    "socket.io": "^4.6.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^20.19.25",
    "@vitest/ui": "^4.0.10",
    "c8": "^10.1.3",
    "prisma": "^5.7.1",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^4.0.10"
  }
}
